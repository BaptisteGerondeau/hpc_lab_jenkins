---
jslaves_benchmark:
{% for item in jslave_list|select("match", ".*bench.*") %}
    - name: {{ item }}
{% if item is match(".*qdc.*") %}
      cgroup: qualcomm
{% elif item is match(".*d0.*") %}
      cgroup: huawei
{% elif item is match(".*tx.*") %}
      cgroup: cavium
{% endif %}
      ssh_key: {{ master_private_key['content'] | b64decode }}

{% endfor %}
